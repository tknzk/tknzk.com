        %li
          %i{class: 'glyphicon glyphicon-ok icon'}
            %a{href: l[:url], target: "_blank"}=l[:title]

%h2
  %legend Skill

%ul{class: 'list-unstyled'}
  - @skills.each do |s|
    %li
      %i{class: 'glyphicon glyphicon-ok icon'}
      =s
%hr

%ul{class: 'list-unstyled'}
  - @waf.each do |w|
    %li
      %i{class: 'glyphicon glyphicon-ok icon'}
      =w


